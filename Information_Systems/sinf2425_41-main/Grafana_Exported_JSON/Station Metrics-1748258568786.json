        {
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \r\n  m.timestamp AS \"time\",\r\n  m.measurement_value AS \"Power (kW)\"\r\nFROM \r\n  dinasore.measurement m\r\nJOIN \r\n  dinasore.sensor s ON m.sensor_id = s.sensor_id\r\nJOIN \r\n  dinasore.station st ON s.station_id = st.station_id\r\nWHERE \r\n  s.tipo = 'Power Consumption Sensor'\r\n  AND st.station_id = $StationID\r\n  AND m.timestamp BETWEEN $__timeFrom() AND $__timeTo()\r\nORDER BY \r\n  m.timestamp;\r\n",
          "refId": "A",
          "sql": {
