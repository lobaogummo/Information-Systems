          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \r\n    \"timestamp\",\r\n    (CASE WHEN s.station_id = 1 THEN m.measurement_value END) AS \"Station 1\",\r\n    (CASE WHEN s.station_id = 2 THEN m.measurement_value END) AS \"Station 2\",\r\n    (CASE WHEN s.station_id = 3 THEN m.measurement_value END) AS \"Station 3\", \r\n    (CASE WHEN s.station_id = 4 THEN m.measurement_value END) AS \"Station 4\",\r\n    (CASE WHEN s.station_id = 5 THEN m.measurement_value END) AS \"Station 5\"\r\nFROM \r\n    dinasore.measurement m\r\nJOIN \r\n    dinasore.sensor s ON m.sensor_id = s.sensor_id\r\nWHERE \r\n    s.tipo = 'Temperature Sensor'\r\n    AND m.timestamp >= $__timeFrom()\r\n    AND m.timestamp <= $__timeTo()\r\nGROUP BY \r\n    m.timestamp,s.station_id,m.measurement_value\r\nORDER BY \r\n  m.timestamp;",
